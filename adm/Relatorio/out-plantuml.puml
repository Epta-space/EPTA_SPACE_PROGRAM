@startuml
skinparam dpi 800
@startuml

                robust "Web Browser" as WB
                concise "Web User" as WU

                @0
                WU is Idle
                WB is Idle

                @1
                WU is Waiting
                WB is Processing

                @3
                WB is Waiting

                @7
                WU is Idle

            @enduml
        
@enduml
